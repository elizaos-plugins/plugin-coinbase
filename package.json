{
    "name": "@elizaos/plugin-coinbase",
    "version": "0.1.8+build.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "github:elizaos-plugins/core",
        "@types/jsonwebtoken": "^9.0.7",
        "coinbase-advanced-sdk": "file:../../packages/plugin-coinbase/advanced-sdk-ts",
        "coinbase-api": "1.0.5",
        "csv-writer": "^1.6.0",
        "jsonwebtoken": "^9.0.2",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "8.3.5",
        "vitest": "^1.0.0"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "eslint --fix  --cache .",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "COINBASE_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Coinbase API key for authentication"
            },
            "COINBASE_PRIVATE_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Coinbase private key for signing requests"
            },
            "COINBASE_COMMERCE_KEY": {
                "type": "string",
                "optional": true,
                "description": "Optional: Coinbase Commerce API key for merchant features"
            }
        }
    }
}
